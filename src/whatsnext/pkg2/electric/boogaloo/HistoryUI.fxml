<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="whatsnext.pkg2.electric.boogaloo.HistoryUIController">
  <children>
    <AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0">
      <children>
        <Button fx:id="returnButton" layoutX="58.0" layoutY="344.0" mnemonicParsing="false" onAction="#showProfileUI" text="Return" />
        <Button fx:id="removeButton" layoutX="245.0" layoutY="344.0" mnemonicParsing="false" onAction="#removeSelectedMedia" text="Remove Selection" />
        <ScrollPane layoutX="58.0" layoutY="44.0" prefHeight="245.0" prefWidth="485.0">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
              <children>
                <TableView fx:id="resultsTable" layoutX="0.0" layoutY="0.0" prefHeight="245.0" prefWidth="485.0">
                  <columns>
                    <TableColumn fx:id="titleColumn" prefWidth="75.0" text="Title" />
                    <TableColumn fx:id="genreColumn" prefWidth="99.0" text="Genre" />
                    <TableColumn fx:id="subGenreColumn" prefWidth="96.0" text="SubGenre" />
                    <TableColumn fx:id="descriptionColumn" prefWidth="214.0" text="Description" />
                  </columns>
                </TableView>
              </children>
            </AnchorPane>
          </content>
        </ScrollPane>
        <Label layoutX="258.0" layoutY="10.0" text="History">
          <font>
            <Font size="25.0" />
          </font>
        </Label>
        <Button fx:id="clearAllButton" layoutX="482.0" layoutY="344.0" mnemonicParsing="false" onAction="#clearAllHistory" text="Clear All" />
      </children>
    </AnchorPane>
  </children>
</AnchorPane>
